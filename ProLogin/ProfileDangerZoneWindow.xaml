<Window x:Class="ProLogin.ProfileDangerZoneWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProLogin"
        mc:Ignorable="d"
        Height="220" Width="400"
        ResizeMode="NoResize">
    <Grid>
        <Button x:Name="profileArchiveButton" Content="Archive Profile" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="profileArchiveButton_Click"/>
        <Button x:Name="profileDeleteButton" Content="Delete Profile" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.895,-2.979" Click="profileDeleteButton_Click"/>

    </Grid>
</Window>
